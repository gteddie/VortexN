	<tr>
		<th>colA</th>
		<th>colB</th>		
	</tr>
</table>
<script>
let array = [1,2,3];
$.ajax({
    type : 'POST',
    url : "/Test/JsonTest",
    data: {array: array},
    success : function(response)
    {
        $("#response").text(response.colA + response.colB);
        let table = {
        		"colA":response.colA,
        		"colB":response.colB
        }
        $("#mytable").DataTable({
        	ajax: "/Test/JsonTest",
        	columns: [
        		{data: 'colA'},
        	    {data: 'colB'}
        	]
        })
		console.log(response);
    },
    error : function(response)
    {
        alert("Error"+response.message);
        console.log('Error');
    }
});
</script>

