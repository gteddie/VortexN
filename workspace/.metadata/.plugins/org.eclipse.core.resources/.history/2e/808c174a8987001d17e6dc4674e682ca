package tw.com.vortex.model.table.dao;

import java.sql.Connection;
import java.util.List;

public class TableDao<T> {
	private T bean;
	private Connection conn;
	public TableDao(T bean,Connection conn) {
		this.bean = bean;
		this.conn = conn;
	}
	public List<T> getBeanList(List<T> beanList,String query) {
		
		return beanList;
	}
}
