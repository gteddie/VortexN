package service;

import java.sql.SQLException;
import java.util.List;

import bean.UserBean;

public interface UserService {
	List<UserBean> insertEmp(List<UserBean> userList) throws SQLException;
	List<UserBean> deleteEmp(int[] userIds) throws SQLException;
	List<UserBean> updateEmp(List<UserBean> userList) throws SQLException;
	List<UserBean> findEmpByNo(int[] userIds) throws SQLException;
	List<UserBean> findAllEmp() throws SQLException;
}
