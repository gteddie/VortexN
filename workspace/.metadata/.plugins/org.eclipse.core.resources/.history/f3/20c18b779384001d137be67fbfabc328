package dao;

import java.sql.SQLException;
import java.util.List;

import bean.UserBean;

public interface UserDao {
	void insertEmp(List<UserBean> empList) throws SQLException;

	void deleteEmpByNo(int[] empnos) throws SQLException;

	void updateEmp(List<UserBean> empList) throws SQLException;

	List<UserBean> findEmpByNo(int[] empnos) throws SQLException;

	List<UserBean> findAllEmp() throws SQLException;
}
